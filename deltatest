    üß® BABFT Full Hell Script by –ü–∞—Ç—Ä–∏–∫ üß®
    ‚ö†Ô∏è –¢–æ–ª—å–∫–æ –¥–ª—è –ø—Ä–∏–≤–∞—Ç–Ω—ã—Ö —Å–µ—Ä–≤–µ—Ä–æ–≤!
    üî• –°–ø–∞–≤–Ω–∏—Ç –∞–¥—Å–∫—É—é –∫—É—á—É –¥–µ—Ç–∞–ª–µ–π + –ª–æ–º–∞–µ—Ç –ª–æ–≥–∏–∫—É –∫–∞—Ä—Ç—ã
    üß† t.me/gde_patrick
--]]

local debris = game:GetService("Debris")
local stages = workspace:WaitForChild("Stages")
local STAGE_CRASH_POS = Vector3.new(0, 10000, 0) -- –≤—Å—ë –≤ –æ–¥–Ω—É —Ç–æ—á–∫—É
local BLOCKS_PER_STAGE = 300 -- –£–≤–µ–ª–∏—á—å –¥–æ 500+ –¥–ª—è —É–ª—å—Ç—Ä–∞ –∫—Ä–∞—à–∞

-- 1. –¢–µ–ª–µ–ø–æ—Ä—Ç –≤—Å–µ—Ö Stage –≤ –æ–¥–Ω—É —Ç–æ—á–∫—É
for _, stage in pairs(stages:GetChildren()) do
    if stage:IsA("Model") then
        task.spawn(function()
            for _, part in pairs(stage:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Anchored = true
                    part.Position = STAGE_CRASH_POS + Vector3.new(math.random(-50, 50), math.random(1, 100), math.random(-50, 50))
                end
            end
        end)
    end
end

wait(1) -- –ñ–¥—ë–º, —á—Ç–æ–±—ã –≤—Å—ë –ø–µ—Ä–µ–º–µ—Å—Ç–∏–ª–æ—Å—å

-- 2. –ê–¥—Å–∫–∏–π —Å–ø–∞–º –±–ª–æ–∫–æ–≤ –≤ –∫—É—á—É
for _, stage in pairs(stages:GetChildren()) do
    for i = 1, BLOCKS_PER_STAGE do
        task.spawn(function()
            local part = Instance.new("Part")
            part.Size = Vector3.new(10, 10, 10)
            part.Position = STAGE_CRASH_POS + Vector3.new(math.random(-100, 100), math.random(1, 300), math.random(-100, 100))
            part.Anchored = false
            part.CanCollide = true
            part.Material = Enum.Material.Neon
            part.BrickColor = BrickColor.Random()
            part.Name = "üíÄ–ê–î"
            part.Parent = workspace

            debris:AddItem(part, 15) -- –£–¥–∞–ª–∏—Ç—Å—è —á–µ—Ä–µ–∑ 15 —Å–µ–∫
        end)
    end
end

print("üî• –ö–†–ê–® –ó–ê–ü–£–©–ï–ù | –ê–≤—Ç–æ—Ä: –ü–∞—Ç—Ä–∏–∫")
